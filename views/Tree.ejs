<html>
	<head>
		<title><%= title %></title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
		<noscript><link rel="stylesheet" href="assets/css/noscript.css" /></noscript>
	</head>
	<body class="is-preload">

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Header -->
					<header id="header">
						<a href="/" class="logo"><%= date %></a>
					</header>

				<!-- Nav -->
				<nav id="nav">
					<ul class="links">
						<li><a href="/">Main</a></li>
						<li class="active"><a href="/Tree">make tree</a></li>
						<li><a href="/WarInfo">war information</a></li>
					</ul>
					<ul class="icons">
						<li><a href=<%= insterurl %> class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
						
					</ul>
				</nav>

				<!-- Main -->
					<div id="main">

						<!-- Post -->
							<section class="post">
								<header class="major">
									<h1>우크라이나에게 보내는 <br/>
									응원 메세지</h1>
									<p>우크라이나 전쟁에 대한 메세지를 써주세요</p>
								</header>
								<div class="image main"><img src="/images/pic01.png" alt="" /></div>
								</section>

					</div>

					<!-- Messages -->
					<div id="messageContainer">
						<% TreeContent.forEach(msg => { %>
						<div id=<%= "msg_"+msg[0] %>>
							<label id=<%= "msg_"+msg[0]+"label" %> for=<%= "msg_"+msg[0]+"textarea" %>> <%= msg[1] %> </label>
							<textarea name=<%= "msg_"+msg[0]+"textarea" %> id=<%= "msg_"+msg[0]+"textarea" %> cols="30" rows="10" i><<%= msg[2] %>/textarea>
						</div>
						<% }) %>
					</div>

				<!-- Footer -->
					<footer id="footer">
						<section>
							<form method="post" action="/Tree/upload">
								<div class="fields">
									<div class="field">
										<label for="name">Name</label>
										<input type="text" name="name" id="name" />
									</div>
									<div class="field">
										<label for="message">Message</label>
										<textarea name="message" id="message" rows="3"></textarea>
									</div>
								</div>
								<ul class="actions">
									<li><input type="submit" value="Send Message" /></li>
								</ul>
							</form>
						</section>
					</footer>

				<!-- Copyright -->
					
				<div id="copyright">
					<ul><li>&copy; <%= footer[0] %></li><li>Creater: <a href="<%= footer[1] %>"><%= footer[2] %></a></li></ul>
				</div>
				

			</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.scrollex.min.js"></script>
			<script src="assets/js/jquery.scrolly.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>

	</body>
</html>